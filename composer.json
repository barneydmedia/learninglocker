    "name": "learninglocker/learninglocker",
    "description": "The open source learning record store.",
    "license": "GPL v3",
    "keywords": ["lrs", "learninglocker"],
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "ext-mongodb": "*",
        "laravel/framework": "5.3.*",
        "jenssegers/mongodb": "3.1.x",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "way/generators": "^3.0",
        "ramsey/array_column": "^1.1",
        "barryvdh/laravel-cors": "^0.8.2",
        "andywer/js-localization": "dev-laravel-5",
        "philo/laravel-translate": "^2.0",
        "learninglocker/statementfactory": "^0.0.10",
        "seld/jsonlint": "^1.5",
        "bshaffer/oauth2-server-php": "^1.8",
        "bshaffer/oauth2-server-httpfoundation-bridge": "^1.2",
        "jenssegers/mongodb-session": "^1.1",
        "league/flysystem": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/flysystem-dropbox": "^1.0",
        "league/flysystem-azure": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "itsgoingd/clockwork": "^1.12",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Http/Controllers",
            "app/locker",
            "app/locker/helpers",
            "app/locker/data",
            "app/locker/graphing",
            "app/locker/explorer",
            "app/locker/statements",
            "app/locker/listeners"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
      
    },
    "config": {
        "secure-http": false,
        "preferred-install": "dist"
    },
    "post-root-package-install": [
        "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize",
      "sh refresh.sh"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize",
      "php artisan clear-compiled",
      "sh refresh.sh"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "minimum-stability": "dev"
}
